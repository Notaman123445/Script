--Made by NOTAMAN123445
local commandEvent = game.ReplicatedStorage.CommandEvent
local actions = {
	"/Kill", --done 
	"/Kick", --done
	"/Invis", --done
	"/Uninvis", --done
	"/Freeze", --done
	"/Unfreeze", --done
	"/Heal", --done
	"/Tp", --done
	"/Farland", --done
	"/Unfarland", --done
	"/Tool",
	"/Nuke",
	"/Npc",
	"/Random",
	"/Blind"
}
-- real command?????????
local function valid_command (action)
	for _, command in pairs(actions) do 
		if command == action then
			return true
		end
	end
end
-- real player???????????
local function valid_player (person)
	local player = game.Players:FindFirstChild(person)
	if player then
		return true
	end
end
--npc command
local function npc_player (person)
	local player = game.Players:FindFirstChild(person)
	local char = player.Character
	local humanoid = char.Humanoid
	humanoid:MoveTo(Vector3.new(10, 3,10))
	wait(5)
	humanoid:MoveTo(Vector3.new(-100, 3, -200))
end
--blind command
local function blind_player (person)
	local players = game.Players
	local player = players:FindFirstChild(person)
	local starterGui = player.PlayerGui
	local blindscreen = starterGui.Blind
	local frame = blindscreen.Frame
	frame.Visible = true
end
--Nuke comamnd
local function nuke_player (person)
	local player = game.Players:FindFirstChild(person)
	local char = player.Character
	local humanoid = char.Humanoid
	humanoid.Health = 0
	wait(3)
	player:Kick("You were kicked becuase admin nuked you")
end
-- kill command
local function kill_player (person)
	local players = game.Players
	local player = players:FindFirstChild(person)
	local char = player.Character
	local hum = char.Humanoid
	hum.Health = 0
end
-- random command
local function random_command (person)
	local ran = math.random(1, 2)
	if ran == 1 then
		kill_player(person)
	else
		print(2)
		nuke_player(person)
	end
end
--farland command
local function farland_player (person)
	local player = game.Players:FindFirstChild(person)
	local char = player.Character
	local HRP = char.HumanoidRootPart
	HRP.CFrame = game.Workspace.FarlandPart.CFrame
end
--unfarland command
local function unfarland_player (person)
	local player = game.Players:FindFirstChild(person)
	player.Character.HumanoidRootPart.CFrame = game.Workspace.PartBack.CFrame
end
--freeze command
local function freeze_player (person)
	local player = game.Players:FindFirstChild(person)
	local humanoid = player.Character.Humanoid
	humanoid.WalkSpeed = 0
	humanoid.JumpPower = 0
end
--unfreeze command
local function unfreeze_player (person)
	local player = game.Players:FindFirstChild(person)
	local humanoid = player.Character.Humanoid
	humanoid.WalkSpeed = 16
	humanoid.JumpPower = 50
end
--tool command
local function tool_player (person)
	local toool = game.ServerStorage.ClassicSword
	local player = game.Players:FindFirstChild(person)
	local backpack = player.Backpack
	local clone = toool:Clone()
	clone.Name = "Sword"
	clone.Parent = backpack
end
-- kick command
local function kick_player (person)
	local player = game.Players:FindFirstChild(person)
	player:Kick("You have been Kicked by a Admin of this game")
end
--healcommand
local function heal_player (person)
	local player = game.Players:FindFirstChild(person)
	player.Character.Humanoid.Health = 100
end
--tp command
local function tp_player (person, person2)
	local player1 = game.Players:FindFirstChild(person)
	local player2 = game.Players:FindFirstChild(person2)
	local player1Char = player1.Character
	local player2Char = player2.Character
	local player1HRP = player1Char.HumanoidRootPart
	local player2HRP = player2Char.HumanoidRootPart
	player1HRP.CFrame = player2HRP.CFrame
end
--Made by NOTAMAN123445
-- invis command
local function invis_player (person)
	local player = game.Players:FindFirstChild(person)
	local plr = player.Character
	for _, limb in pairs(plr:GetChildren()) do		
		if limb:IsA("BasePart")	then
			limb.Transparency = 1
			if limb.Name == "Head" then
				limb.face.Transparency = 1
			end
		elseif limb:IsA("Accessory") then
			limb.Handle.Transparency = 1
		end
	end
end
-- uninvis command
local function uninvis_player (person)
	local player = game.Players:FindFirstChild(person)
	local plr = player.Character
	for _, limb in pairs(plr:GetChildren()) do
		if limb:IsA("BasePart")	then
			limb.Transparency = 0
			if limb.Name == "Head" then
				limb.face.Transparency = 0
			elseif limb.Name == "HumanoidRootPart" then
				limb.Transparency = 1
			end
		elseif limb:IsA("Accessory") then
			limb.Handle.Transparency = 0
		end		
	end
end
-- main
local function do_command (player, action, person, person2)
	if valid_command(action) and valid_player(person) then
		if action == "/Kill" then
			kill_player(person)
		end
		if action == "/Kick" then
			kick_player(person)
		end
		if action == "/Invis" then
			invis_player(person)
		end
		if action == "/Uninvis" then
			uninvis_player(person)
		end
		if action == "/Freeze" then
			freeze_player(person)
		end
		if action == "/Unfreeze" then
			unfreeze_player(person)
		end
		if action == "/Heal" then
			heal_player(person)
		end
		if action == "/Tp" then
			tp_player(person, person2)
		end
		if action == "/Farland" then
			farland_player(person)
		end
		if action == "/Unfarland" then
			unfarland_player(person)
		end
		if action == "/Tool" then
			tool_player(person)
		end
		if action == "/Nuke" then
			nuke_player(person)
		end
		if action == "/Npc" then
			npc_player(person)
		end
		if action == "/Random" then
			random_command(person)
		end
		if action == "/Blind" then
			blind_player(person)
		end
	else
		print("error with admin commands")
		warn("Error Admin Commands")		
	end
end
commandEvent.OnServerEvent:Connect(do_command)
--Made by NOTAMAN123445
